import random
dlzka = int(input("Ako dlhé heslo: "))
samohlasky = "aeiouy"
spoluhlasky = "qwrtpsdfghjklzxcvbnm"
nr = ""
for i in range(0,dlzka):
    if i%2 == 0:
        nr += random.choice(samohlasky)
    else:
        nr += random.choice(spoluhlasky)
print(nr)


