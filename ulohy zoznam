# napis program ktorý nacita lubovolne slovo a vytvori zoznam ktory bude obsahovat ordinalne hodnoty jeho znakov

a = input("Napíš premennú: ")
zoz = []
for i in range(0,len(a)):
    zoz.append(ord(a[i]))
print(zoz)


# napis program ktorý nacita lubovolne cisla a premenim ich do str
import random
zoz3 = []
for i in range(0,10):
    zoz3.append(random.randrange(0,10000))
print(zoz3)

vstup = ""
for i in zoz3:
    vstup += chr(i)
print(vstup)
